; Web server/service configuration
APP_NAME=auto-generated-app
PORT=5000
HTTPS_ENABLE=false
HTTPS_PRIVATE_KEY=key.pem
HTTPS_CERT=cert.pem
CORS_ENABLED=true
CORS_ORIGIN=http://localhost:3000
SESSION_TIMEOUT=86400
API_PREFIX=api
WEB_DISABLED=false
STATIC_CONTENT_PATH=Public
NOTIFICATION_TASK_ENABLED=true
TASK_OR_JOB_TIME_LOG_ENABLED=true

; Database connection string
DATABASE=mongodb://localhost:27017/testing

; Logger Configuration
LOGGER_MAX_FILE_SIZE=10
LOGGER_LEVEL=info
LOGGER_API_LOG_ENABLE=true

; SMTP Configuration for Email
SMTP_HOST=xyz.com
SMTP_PORT=587
SMTP_USERNAME=xyz@xyz.com
SMTP_PASSWORD=123456
SMTP_SECURE=false
SMTP_DEFAULT_EMAIL=xyz@xyz.com
SMTP_IGNORE_TLS=true

; Text Server configuration
TWILIO_ACCOUNT_SID=xyz
TWILIO_AUTH_TOKEN=xyz

; SSO configuration for Google
GOOGLE_CLIENT_ID=xyz.apps.googleusercontent.com
GOOGLE_SECRATE=xyz
GOOGLE_REDIRECTURI=http://localhost:5000/api/google
GOOGLE_APP_REDIRECT=/
GOOGLE_AUTH_FAIL_REDIRECT=/Login?error=User not registered